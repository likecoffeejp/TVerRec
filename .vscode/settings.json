{
	"[dockercompose]": {
		"editor.autoIndent": "advanced"
	},
	"[jsonc]": {
		"editor.autoIndent": "advanced",
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[markdown]": {
		"editor.autoIndent": "advanced",
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[powershell]": {
		"editor.autoIndent": "advanced",
		"editor.defaultFormatter": "ms-vscode.powershell"
	},
	"[xml]": {
		"editor.autoIndent": "advanced",
		"editor.defaultFormatter": "rogalmic.vscode-xml-complete"
	},
	"[yaml]": {
		"editor.autoIndent": "advanced"
	},
	"diffEditor.codeLens": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.autoIndent": "advanced",
	"editor.bracketPairColorization.enabled": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.detectIndentation": true,
	"editor.foldingImportsByDefault": true,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.hover.sticky": false,
	"editor.insertSpaces": false,
	"editor.renderControlCharacters": true,
	"editor.renderWhitespace": "all",
	"files.autoGuessEncoding": true,
	"files.autoSave": "afterDelay",
	"files.trimTrailingWhitespace": true,
	"powershell.buttons.showPanelMovementButtons": true,
	"powershell.codeFormatting.addWhitespaceAroundPipe": true,
	"powershell.codeFormatting.autoCorrectAliases": true,
	"powershell.codeFormatting.newLineAfterCloseBrace": false,
	"powershell.codeFormatting.newLineAfterOpenBrace": false,
	"powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationForFirstPipeline",
	"powershell.codeFormatting.useConstantStrings": true,
	"powershell.codeFormatting.useCorrectCasing": true,
	"powershell.codeFormatting.whitespaceBetweenParameters": true,
	"powershell.integratedConsole.forceClearScrollbackBuffer": true,
	"powershell.integratedConsole.suppressStartupBanner": true,
	"powershell.scriptAnalysis.enable": true,
	"powershell.scriptAnalysis.settingsPath": "./.vscode/PSScriptAnalyzerSettings.psd1",
	"powershell.startAsLoginShell.linux": true,
	"prettier.bracketSameLine": true,
	"prettier.endOfLine": "auto",
	"prettier.useTabs": true,
	"thunder-client.saveToWorkspace": true,
	"thunder-client.saveTokenPerEnvironment": true,
	"thunder-client.sidebar.defaultTab": "Collections",
	"thunder-client.sidebar.showActivityFromCollection": true,
	"thunder-client.workspaceRelativePath": "./.vscode/",
	"workbench.iconTheme": "vscode-icons",
	"workbench.preferredLightColorTheme": "Default Dark+",
	"workbench.startupEditor": "newUntitledFile",
	"gitlens.graph.dimMergeCommits": true,
	"workbench.colorTheme": "Default Dark+"
}
